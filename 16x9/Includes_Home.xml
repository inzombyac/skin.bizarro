<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="Furniture_RSS">
		<control type="group">
			<visible>!Player.HasMedia</visible>
			<visible>system.getbool(lookandfeel.enablerssfeeds)</visible>
			<visible>!String.Contains(Window(10025).Property(PlayingBackgroundMedia),True)</visible>
			<control type="rss">
				<description>RSS feed</description>
				<left>80</left>
				<top>0</top>
				<height>30</height>
				<width>1760</width>
				<font>font12</font>
				<urlset>1</urlset>
				<textcolor>$VAR[ColourHighlight2]</textcolor>
				<titlecolor>$VAR[ColourHighlight]</titlecolor>
				<headlinecolor>white</headlinecolor>
			</control>
		</control>
	</include>
	<include name="DebugGridHome">
		 <control type="group">
			<visible>!Window.IsVisible(DialogVideoInfo.xml) + !Window.IsVisible(DialogMusicInfo.xml)</visible>
			 <visible>Skin.HasSetting(DebugGrid)</visible>
			<control type="label">
                <include>ControlID2</include>
                <label>$INFO[Container(4019).ListItem.Path]</label>
				<visible>Control.HasFocus(4012)</visible>
            </control>
			<control type="label">
                <include>ControlID2</include>
                <label>$INFO[Container(4016).ListItem.Path]</label>
				<visible>Control.HasFocus(4016)</visible>
            </control>
			<control type="label">
                <include>ControlID2</include>
                <label>$INFO[Window(Home).Container(4017).ListItem.Path]</label>
				<visible>Control.HasFocus(4017)</visible>
            </control>
			<control type="label">
                <include>ControlID2</include>
                <label>$INFO[Container(4018).ListItem.Path]</label>
				<visible>Control.HasFocus(4018)</visible>
            </control>
			<control type="label">
                <include>ControlID2</include>
                <label>$INFO[Container(300).ListItem.Property(Path)]</label>
				<visible>Control.HasFocus(300)</visible>
            </control>
			<control type="label">
                <include>ControlID2</include>
                <label>$INFO[Container(302).ListItem.Property(Path)]</label>
				<visible>Control.HasFocus(302)</visible>
            </control>
			<control type="label">
                <include>ControlID3</include>
                <label>$VAR[EmbyHomeDBType] $VAR[EmbyHomeTVShowID]</label>
            </control>
			<control type="label">
                <include>ControlID5</include>
                <label>DBID: $INFO[ListItem.DBID],$INFO[ListItem.Property(DBID)]</label>
				
            </control>
			
		</control>
	</include>
</includes>