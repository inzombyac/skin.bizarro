						<left>373</left>
						<centertop>30</centertop>
						<height>40</height>
						<width>50</width>
						<align>center</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Property(UnwatchedEpisodes)]</label>
						<font>Small</font>
						<textcolor>White</textcolor>
					</control>
				</control>
			</control>
