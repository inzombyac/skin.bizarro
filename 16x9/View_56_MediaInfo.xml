		<control type="label">
			<posx>30</posx>
			<width>auto</width>
			<aligny>center</aligny>
			<height>64</height>
			<font>Flag</font>
			<label>$INFO[ListItem.Label]</label>
			<textcolor>Dark3</textcolor>
			<selectedcolor>Dark3</selectedcolor>
			<visible>String.IsEqual(ListItem.Overlay,OverlayWatched.png)</visible>
