		</control>
		<control type="progress">
			<centerright>100</centerright>
			<centertop>32</centertop>
			<width>70</width>
			<height>19</height>
			<reveal>true</reveal>
			<texturebg>pvr/progress-back.png</texturebg>
			<lefttexture colordiffuse="EmbyGreen">pvr/progress-leftright.png</lefttexture>
