   <shortcut>
        <label>$LOCALIZE[1036]</label>
        <label2>favorite</label2>
        <icon>special://skin/extras/icons/favorite.png</icon>
        <thumb>special://skin/extras/backgrounds/favorites.jpg</thumb>
        <action>ActivateWindow(Favourites)</action>
        <visible>!System.HasAddon(plugin.video.emby) + [Integer.IsGreater(Window(Home).Property(TVShows.Count),0) | Integer.IsGreater(Window(Home).Property(Movies.Count),0) | Integer.IsGreater(Window(Home).Property(Music.SongsCount),0)]</visible>
    </shortcut>
