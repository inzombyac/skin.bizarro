<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Recommended Movies All</name>
    <match>all</match>
	<rule field="rating" operator="greaterthan">
		<value>7</value>
	</rule>
    <limit>25</limit>
    <order direction="descending">rating</order>
</smartplaylist>
